// Base.xcconfig
// Shared configuration for all build configurations

// Dynamic bundle identifier using reverse DNS with current user
PRODUCT_BUNDLE_IDENTIFIER_PREFIX = com.${USER:rfc1034identifier}
PRODUCT_NAME = ZIPServeKitExample

// Automatic code signing
CODE_SIGN_STYLE = Automatic

// Let Xcode manage provisioning
DEVELOPMENT_TEAM =

// Marketing version
MARKETING_VERSION = 1.0
CURRENT_PROJECT_VERSION = 1
